[run]
source = agents,api,gui,SHARED
branch = True
omit = 
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */env/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

show_missing = True
precision = 2
fail_under = 70

[html]
directory = htmlcov
title = AI Agent League - Coverage Report

[xml]
output = coverage.xml

[json]
output = coverage.json
pretty_print = True
