{
  "max_concurrent_matches": 5,
  "logging_level": "INFO",
  "auto_report_results": true,
  "match_orchestration": {
    "auto_forfeit_on_timeout": true
  },
  "state_machine": {
    "initial_state": "INIT",
    "states": ["INIT", "REGISTERED", "ACTIVE", "SUSPENDED", "SHUTDOWN"],
    "transitions": {
      "INIT": ["REGISTERED", "SHUTDOWN"],
      "REGISTERED": ["ACTIVE", "SUSPENDED", "SHUTDOWN"],
      "ACTIVE": ["SUSPENDED", "SHUTDOWN"],
      "SUSPENDED": ["ACTIVE", "SHUTDOWN"],
      "SHUTDOWN": []
    }
  }
}
