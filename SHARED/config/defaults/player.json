{
  "default_strategy": "random",
  "logging_level": "INFO",
  "strategy_settings": {
    "random": {
      "seed": null
    },
    "frequency": {
      "min_history_size": 3,
      "counter_bias": 0.8
    },
    "pattern": {
      "pattern_length": 3,
      "min_confidence": 0.6
    }
  },
  "state_machine": {
    "initial_state": "INIT",
    "states": ["INIT", "REGISTERED", "ACTIVE", "SUSPENDED", "SHUTDOWN"],
    "transitions": {
      "INIT": ["REGISTERED", "SHUTDOWN"],
      "REGISTERED": ["ACTIVE", "SUSPENDED", "SHUTDOWN"],
      "ACTIVE": ["SUSPENDED", "SHUTDOWN"],
      "SUSPENDED": ["ACTIVE", "SHUTDOWN"],
      "SHUTDOWN": []
    }
  }
}
